<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>1 OR 2</booleanFilter>
    <name>US-Northeast State Code</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingStateCode</field>
        <operation>equals</operation>
        <value>CT,ME,MA,NH,NJ,NY,PA,RI,VT</value>
    </ruleItems>
    <ruleItems>
        <field>Account.ShippingStateCode</field>
        <operation>equals</operation>
        <value>CT,ME,MA,NH,NJ,NY,PA,RI,VT</value>
    </ruleItems>
</Territory2Rule>
